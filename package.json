{
  "name": "courseware_web_project",
  "version": "4.4.0",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "Pan <panfree23@gmail.com>",
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build:prod": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "build:stage": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode staging",
    "preview": "NODE_OPTIONS=--openssl-legacy-provider node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "vue-axios": "^3.3.4",
    "core-js": "^3.25.5",
    "echarts": "^5.4.0",
    "element-ui": "2.15.12",
    "js-cookie": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "6.2.1",
    "vue": "2.7.10",
    "vue-router": "3.6.5",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.19",
    "@vue/cli-plugin-unit-jest": "4.5.19",
    "@vue/cli-service": "4.5.19",
    "@vue/test-utils": "1.3.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.1.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "4.1.2",
    "connect": "3.7.0",
    "eslint-plugin-vue": "8.7.1",
    "html-webpack-plugin": "4.5.2",
    "sass": "1.55.0",
    "sass-loader": "^10.1.1",
    "script-ext-html-webpack-plugin": "2.1.5",
    "serve-static": "1.15.0",
    "svg-sprite-loader": "6.0.11",
    "svgo": "3.0.2",
    "vue-template-compiler": "2.7.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "license": "MIT"
}